<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.shopzilla.perf.data">

    <class name="PerfData" table="perfdata">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="methodName" type="string" column="method_name"/>
        <property name="invokeTime" type="timestamp" column="invoke_time"/>
        <property name="execTime" type="long" column="exec_time"/>
    </class>

</hibernate-mapping>
